{
  "name": "agency-chat-backend-util",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/agency-chat-backend/util/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/agency-chat-backend/util"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/agency-chat-backend/util"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/agency-chat-backend/util"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/agency-chat-backend/util/**/*.ts"]
      }
    }
  },
  "tags": []
}
